{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "tutor.html", "use": "@vercel/static" },
    { "src": "tutor.js", "use": "@vercel/static" },
    { "src": "tutor.css", "use": "@vercel/static" },
    { "src": "config.js", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(.*.(css|js|png|jpg|jpeg|gif|svg|ico|webp))",
      "dest": "/$1"
    },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/tutor.html" }
  ]
}
